# Place this file in your /etc/yum.repos.d/ directory

# Community Releases
[pulp]
name=Pulp Community Releases
baseurl=http://repos.fedorapeople.org/repos/pulp/pulp/$releasever/$basearch/
enabled=1
skip_if_unavailable=1
gpgcheck=0

