# Place this file in your /etc/yum.repos.d/ directory

[thumbslug]
name=An open source content proxy
baseurl=http://repos.fedorapeople.org/repos/candlepin/thumbslug/epel-$releasever/$basearch/
enabled=1
skip_if_unavailable=1
gpgcheck=0

[thumbslug-source]
name=An open source content proxy. - Source
baseurl=http://repos.fedorapeople.org/repos/candlepin/thumbslug/epel-$releasever/SRPMS
enabled=0
skip_if_unavailable=1
gpgcheck=0

